@model LoginViewModel
@{
    ViewData["Title"] = "Login";
}

<h1>Login</h1>
<form method="post">
    <div asp-validation-summary="ModelOnly"></div>
   Name <input asp-for="Name" /> <span asp-validation-for="Name" /> <br />
   Password <input asp-for="Password" /> <span asp-validation-for="Password" /><br />
    <input type="submit" value="login"/>

</form>
